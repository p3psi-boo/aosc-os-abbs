abinfo "Building amd64 efi binary and bios binary..."
cd build64
make 

abinfo "Installing 64 bit efi binary and bios binary..."
install -Dm644 memtest.bin "$PKGDIR"/boot/memtest86plus/memtest32.bin
install -Dm644 memtest.efi "$PKGDIR"/boot/memtest86plus/memtest64.efi

abinfo "Building ia32 efi binary..."
cd "$SRCDIR"/build32
make memtest.efi

abinfo "Installing ia32 efi binary..."
install -Dm644 memtest.efi "$PKGDIR"/boot/memtest86plus/memtest32.efi
